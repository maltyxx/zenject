{
  "name": "@maltyxx/zenject",
  "version": "1.0.0",
  "description": "A lightweight dependency injection framework for Bun.js applications",
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./testing": "./src/testing/index.ts",
    "./tokens": "./src/tokens/common-tokens.ts",
    "./cli": "./src/cli/index.ts"
  },
  "bin": {
    "zenject": "./src/cli/index.ts"
  },
  "types": "src/index.ts",
  "scripts": {
    "docs": "typedoc --options typedoc.json",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "prepare": "chmod +x ./src/cli/index.ts"
  },
  "keywords": [
    "bun",
    "dependency-injection",
    "di",
    "ioc",
    "typescript"
  ],
  "author": "maltyxx",
  "license": "MIT",
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.1",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "typedoc": "^0.28.3",
    "typedoc-plugin-markdown": "^4.0.0-next.36",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
